    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
